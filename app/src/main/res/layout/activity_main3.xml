<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView5"
            android:layout_width="221dp"
            android:layout_height="32dp"
            android:text="Nombre de Usuario"
            tools:layout_editor_absoluteX="69dp"
            tools:layout_editor_absoluteY="449dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="221dp"
            android:layout_height="32dp"
            android:text="Contraseña"
            tools:layout_editor_absoluteX="69dp"
            tools:layout_editor_absoluteY="580dp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="221dp"
            android:layout_height="32dp"
            android:text="Confirmar Contraseña"
            tools:layout_editor_absoluteX="69dp"
            tools:layout_editor_absoluteY="711dp" />

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/editText3"
            android:layout_width="571dp"
            android:layout_height="58dp"
            android:hint="hint"
            tools:layout_editor_absoluteX="69dp"
            tools:layout_editor_absoluteY="492dp" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="203dp"
            android:layout_height="198dp"
            tools:layout_editor_absoluteX="253dp"
            tools:layout_editor_absoluteY="86dp"
            tools:srcCompat="@tools:sample/avatars" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="198dp"
            android:layout_height="42dp"
            android:text="Registro de secion"
            tools:layout_editor_absoluteX="294dp"
            tools:layout_editor_absoluteY="37dp" />

        <Button
            android:id="@+id/button"
            android:layout_width="137dp"
            android:layout_height="70dp"
            android:text="Cancelar"
            tools:layout_editor_absoluteX="189dp"
            tools:layout_editor_absoluteY="855dp" />

        <Button
            android:id="@+id/button2"
            android:layout_width="154dp"
            android:layout_height="75dp"
            android:text="Aceptar"
            tools:layout_editor_absoluteX="403dp"
            tools:layout_editor_absoluteY="855dp" />

        <com.google.android.material.textfield.TextInputEditText
            android:layout_width="571dp"
            android:layout_height="51dp"
            android:hint="hint"
            tools:layout_editor_absoluteX="69dp"
            tools:layout_editor_absoluteY="369dp" />

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/editText"
            android:layout_width="571dp"
            android:layout_height="58dp"
            android:hint="hint"
            tools:layout_editor_absoluteX="69dp"
            tools:layout_editor_absoluteY="628dp" />

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/editText2"
            android:layout_width="571dp"
            android:layout_height="59dp"
            android:hint="hint"
            tools:layout_editor_absoluteX="69dp"
            tools:layout_editor_absoluteY="756dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="221dp"
            android:layout_height="32dp"
            android:text="Correo electronico"
            tools:layout_editor_absoluteX="69dp"
            tools:layout_editor_absoluteY="326dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>